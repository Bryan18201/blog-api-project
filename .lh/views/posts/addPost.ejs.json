            "content": "<!DOCTYPE html>\n<html lang=\"en\">\n  <head>\n    <meta charset=\"UTF-8\" />\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\" />\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\n    <!-- Fontawesome -->\n    <script\n      src=\"https://kit.fontawesome.com/42a182743f.js\"\n      crossorigin=\"anonymous\"\n    ></script>\n    <!-- CSS -->\n    <link rel=\"stylesheet\" href=\"/public/css/navbar.css\" />\n    <link rel=\"stylesheet\" href=\"/css/form.css\" />\n    \n    <title>Post Creation</title>\n  </head>\n  <body>\n    <!-- nav -->\n    <%-include('../partials/navbar.ejs') %>\n    <div class=\"form-container\">\n      <!-- icon -->\n      <h1>\n        Add Post\n\n        <!-- add icon -->\n        <i class=\"fas fa-plus\"></i>\n      </h1>\n      <%if(error){%>\n      <small class=\"error\">\n        <i class=\"fas fa-exclamation-circle\"></i>\n        <%=error%>\n      </small>\n      <%}%>\n      <form enctype=\"multipart/form-data\" action=\"/api/v1/posts\" method=\"POST\">\n        <!-- Post Title -->\n        <div class=\"input-wrapper\">\n          <label for=\"name\">Post Title</label>\n          <input type=\"text\" name=\"title\" id=\"name\" />\n          <!-- err -->\n        </div>\n\n        <!-- Category -->\n        <div class=\"input-wrapper\">\n          <label for=\"category\">Category</label>\n          <select name=\"category\" id=\"category\">\n            <option value=\"react js\">React js</option>\n            <option value=\"html\">HTML</option>\n            <option value=\"css\">CSS</option>\n            <option value=\"node js\">Node js</option>\n            <option value=\"javascript\">Javascript</option>\n            <option value=\"javascript\">Python</option>\n            <option value=\"javascript\">Python</option>\n            <option value=\"other\">Other</option>\n          </select>\n          <!-- err -->\n        </div>\n        <div class=\"input-wrapper\">\n          <!-- Post Body -->\n          <label for=\"body\">Post Body</label>\n          <textarea name=\"description\" id=\"body\" cols=\"30\" rows=\"10\"></textarea>\n          <!-- err -->\n        </div>\n        <!-- Post Image -->\n        <div class=\"input-wrapper\">\n          <label for=\"image\">Post Image</label>\n          <input type=\"file\" name=\"file\" id=\"image\" />\n          <!-- err -->\n        </div>\n\n        <button type=\"submit\">Create</button>\n      </form>\n    </div>\n    <!-- footer -->\n    <%-include('../partials/footer.ejs') %>\n  </body>\n</html>\n"
